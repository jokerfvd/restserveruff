<%= form_for(@estabelecimento) do |f| %>
  <% if @estabelecimento.errors.any? %>
  <div id="errorExplanation">
    <h2><%= pluralize(@estabelecimento.errors.count, "error") %> o estabelecimento nao pode ser salvo:</h2>
    <ul>
    <% @estabelecimento.errors.full_messages.each do |msg| %>
      <li><%= msg %></li>
    <% end %>
    </ul>
  </div>
  <% end %>
 
  <div class="field">
    <%= f.label :nome %><br />
    <%= f.text_field :nome %>
  </div>
  <div class="field">
    <%= f.label :endereco %><br />
    <%= f.text_field :endereco %>
  </div>
  <div class="field">
    <%= f.label :telefone %><br />
    <%= f.text_field :telefone %>
  </div>
  <div class="field">
    <%= f.label :rank %><br />
    <%= f.text_field :rank %>
  </div>
  <div class="actions">
    <%= f.submit "ok"%>
  </div>
<% end %>